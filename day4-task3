n = int(input("Enter the no. of elements of dictionary : "))
li = []
diction = { }
for i in range(1,n+1):
    element = int(input("Enter the "+str(i)+" element of dictionary"))
    diction[i] = element
    li.append(element)
s = set(li)
li1 = list(s)
li1.sort()
print("your input dictionary is : ")
print(diction)
print("second maximum value of array is")
print(li1[-2])
